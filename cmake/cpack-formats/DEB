#
# CPack package generation
#
SET(CPACK_DEBIAN_FILE_NAME "DEB-DEFAULT")
IF (AUTO_TMPDIR_CPACK_IGNORE_SLURM_VERSION)
    SET(CPACK_DEBIAN_PACKAGE_DEPENDS "slurmd")
ELSE ()
    SET(CPACK_DEBIAN_PACKAGE_DEPENDS "slurmd(>= ${SLURM_VERSION_MAJOR_MINOR_ONLY}), slurmd(<< ${SLURM_VERSION_MAJOR_MINOR_ONLY}.999)")
    SET(CPACK_DEBIAN_PACKAGE_RELEASE "${SLURM_VERSION}")
ENDIF ()

